{
  "name": "baby-steps-app",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "frontend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/food/(.*)",
      "dest": "/frontend/api/food/$1"
    },
    {
      "src": "/api/meal/(.*)",
      "dest": "/frontend/api/meal/$1"
    },
    {
      "src": "/api/emergency/(.*)",
      "dest": "/frontend/api/emergency/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/frontend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_ADSENSE_CLIENT_ID": "ca-pub-1934622676928053"
  },
  "functions": {
    "frontend/api/**/*.js": {
      "maxDuration": 10
    }
  }
}